<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=0.9  " />
    <title>@ViewData["Title"]  Cat Page</title>

    <partial name="_Preloader" />

    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/dist/css/bootstrap-grid.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/dist/css/bootstrap-reboot.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/site.css" />


</head>
<body>
    <header>
        <nav class="navbar navbar-expand-md navbar-toggler navbar-light bg-white">
            <div class="container-fluid">
                <img class="ml-5" src="~/resources/LOGO_64.png" />

                <a id="BRAND" class="navbar-brand " asp-area="" asp-controller="Home" asp-action="Index">
                    <h3>Cat Page</h3>
                </a>

                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="navbar-collapse collapse d-md-inline-flex flex-md-row-reverse ">

                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
    <div class="MainContainer">
        <partial name="_CookieConsentPartial" />

        <main id="Main-Content" role="main">
            @RenderBody()
        </main>
    </div>
    <div style="text-align:center">
        <div class="btn-group">
            <button id="IncreasePageButton" class="btn btn-primary">+</button>
            <button id="DecreasePageButton" class="btn btn-primary">-</button>
        </div>

    </div>
    <footer class="border-top footer text-muted">
        <div class="container ">

            <div style="color:black">
                &copy; 2019 - Colt Company
            </div>

        </div>
    </footer>

    <script>
        var User = '@User.Identity.Name';
        var Owner = '@ViewBag.person';
        function isYourPage() {
            return User == Owner;
        }
    </script>
    <script src="https://code.jquery.com/jquery-3.4.1.js"
            integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU="
            crossorigin="anonymous"></script>
    <script src="~/lib/jquery-ui-1.12.1/jquery-ui.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>

    <script src="~/js/site.js" asp-append-version="true"></script>

    <script src="~/js/3_Main/device.min.js"></script>

    <script src="~/js/7_Modal/ModalBlockControlScript.js"></script>


    <script src="~/js/2_Arhitecture/EditBlockFrameScriptt.js"></script>


    <script src="~/js/4_Components/DynamicInputFieldScript.js"></script>

    <script src="~/js/6_Mobile/UITouchPunchScript.js"></script>
    <script src="~/js/6_Mobile/MobileEvents.js"></script>

    @RenderSection("Scripts", required: false)



    <script src="~/js/1_Pages/EditPageMenu.js"></script>
    <script src="~/js/1_Pages/DefaultPageScript.js"></script>
    <script src="~/js/1_Pages/Events/DefaultPageEventsBind.js"></script>

    <script src="~/js/2_Arhitecture/AdaptivePersonalPageScript.js"></script>

</body>
</html>